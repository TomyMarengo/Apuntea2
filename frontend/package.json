    "@reduxjs/toolkit": "^2.5.0",
    "@tanstack/react-query": "^5.62.10",
    "@testing-library/react": "^16.1.0",
    "autoprefixer": "^10.4.20",
    "dayjs": "^1.11.13",
    "file-saver": "^2.0.5",
    "i18next": "^24.2.0",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
